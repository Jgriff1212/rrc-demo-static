# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Edge Functions URL (usually same as Supabase URL)
EXPO_PUBLIC_FUNCTIONS_URL=https://your-project.supabase.co/functions/v1

# Image Provider Configuration
# Options: 'mock' | 'http'
EXPO_PUBLIC_IMAGE_PROVIDER=mock
EXPO_PUBLIC_IMAGE_PROVIDER_URL=https://api.example.com/generate
EXPO_PUBLIC_IMAGE_PROVIDER_API_KEY=your-api-key

# RevenueCat Configuration (stub)
EXPO_PUBLIC_REVENUECAT_API_KEY=your-revenuecat-key

# Feature Flags
EXPO_PUBLIC_ENABLE_REVEAL_REAL=true
EXPO_PUBLIC_ENABLE_LEADERBOARD=true

# Development
EXPO_PUBLIC_DEV_MODE=true
